<div class= "add-friend">
  <% if @friendship.receiver.photo.attached? %>
    <%= cl_image_tag @friendship.receiver.photo.key, class:"avatar-leaderboard" %>
  <% else %>
    <%= image_tag("https://res.cloudinary.com/dvvbk1j65/image/upload/v1679233516/download_riwhuo.png", class:"avatar-leaderboard") %>
  <% end %>
  <h3><%= @friendship.receiver.username %></h3>
  <i class="fa-solid fa-check"></i>
</div>
